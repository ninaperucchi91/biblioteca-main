<% if(locals.message.titulo) { %>

<div class="container mt-2">
  <div class="row">
    <div class="col-md-12">
      <div class="bs-component">
        <div class="alert alert-dismissible alert-<%=message.type%>">
          <button
            type="button"
            class="btn-close"
            data-bs-dismiss="alert"
          ></button>
          <h4 class="alert-heading"><%=message.titulo %> !</h4>
          <p class="mb-0"><%=message.body %></p>
        </div>
      </div>
    </div>
  </div>
</div>

<% } %>
