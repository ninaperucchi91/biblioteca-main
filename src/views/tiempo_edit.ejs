<%- include("partials/_header") -%> <%- include("partials/_navbar") -%>

<div class="container mt-5">
  <h3>Editar Tiempo Prestamo</h3>
  <div class="row">
    <div class="col-md-5">
      <div class="card">
        <div class="card-body">
          <form action="/tiempo/edit/<%= data.idTiempoEstadia %>"   method="POST" >
            <div class="form-group mt-2">
              <input
                type="text"
                name="TiempoEstadia"
                placeholder="Tiempo"
                class="form-control"
                value="<%= data.TiempoEstadia %>"
              />
            </div>
            <div class="form-group mt-2">
              <input
                type="text"
                name="descripcion"
                placeholder="Descripcion"
                class="form-control"
                value="<%= data.descripcion %>"
              />
            </div>
            <div class="form-group mt-2">
              <button type="submit" class="btn btn-primary">GUARDAR</button>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>

<%- include("partials/_footer") -%>
